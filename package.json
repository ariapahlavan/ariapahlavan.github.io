{
  "name": "gh-pages",
  "version": "0.0.0",
  "scripts": {
    "prebuild:watch": "npm run lite-server &",
    "build:watch": "ng build --watch --base-href /site",
    "postbuild:watch": "kill (ps | grep 'lite-server' | head -n1 | awk '{print $1;}')",
    "lite-server": "lite-server --baseDir=dist/gh-pages",
    "add-404": "cp docs/site/index.html docs/site/404.html && cp docs/site/index.html docs/404.html",
    "ci": "ng build --configuration production --output-path docs/site --base-href /site/",
    "postci": "npm run add-404 && touch docs/site/.nojekyll",
    "gen-post": "./post_gen.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.2",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "~13.2.2",
    "@angular/compiler": "~13.2.2",
    "@angular/core": "~13.2.2",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~13.2.2",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/router": "~13.2.2",
    "core-js": "^2.5.4",
    "material-design-icons": "^3.0.1",
    "ngx-markdown": "^11.1.2",
    "ngx-webstorage-service": "^4.1.0",
    "prismjs": "^1.23.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.2",
    "@angular/language-service": "~13.2.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lite-server": "^2.6.1",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5"
  }
}